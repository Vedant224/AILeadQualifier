{
  "name": "lead-scoring-backend",
  "version": "1.0.0",
  "description": "Backend service for lead qualification using rule-based scoring and AI-powered intent analysis",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "deploy": "bash scripts/deploy.sh",
    "health-check": "bash scripts/health-check.sh",
    "test:deployment": "bash scripts/test-deployment.sh",
    "docker:build": "bash scripts/docker-build.sh build",
    "docker:run": "bash scripts/docker-build.sh run",
    "docker:dev": "bash scripts/docker-build.sh run-dev",
    "docker:stop": "bash scripts/docker-build.sh stop",
    "docker:logs": "bash scripts/docker-build.sh logs",
    "prestart": "npm run build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "lead-scoring",
    "api",
    "ai",
    "backend",
    "typescript"
  ],
  "author": "Backend Engineer Candidate",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
