{
  "version": 2,
  "name": "lead-scoring-backend",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "info",
    "MAX_FILE_SIZE": "10485760",
    "MAX_LEADS_PER_UPLOAD": "1000"
  },
  "regions": ["iad1"],
  "buildCommand": "npm run build"
}